# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_API_BASE_URL=https://your-api-gateway-url.amazonaws.com/prod
VITE_AWS_COGNITO_USER_POOL_ID=your-cognito-user-pool-id
VITE_AWS_COGNITO_CLIENT_ID=your-cognito-client-id
VITE_AWS_S3_BUCKET=your-s3-bucket-name

# Feature Flags
VITE_USE_AMPLIFY_AUTH=false
VITE_USE_AWS_S3=false
VITE_ENABLE_LOVABLE_FEATURES=false

# External Integrations (Optional)
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_RAZORPAY_KEY_ID=your-razorpay-key

# Development/Testing
VITE_API_TIMEOUT=30000
VITE_DEBUG_MODE=false